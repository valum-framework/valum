prefix=@prefix@
exec_prefix=@exec_prefix@
libdir=@libdir@
includedir=@includedir@

Name: Valum
Description: Web micro-framework written in Vala
URL: https://github.com/valum-framework/valum
Version: @VERSION@
Requires: glib-2.0 gio-2.0 libsoup-2.4 gee-0.8 ctpl
Libs: -L${libdir} -lvalum-@API_VERSION@
Libs.private: -L${libdir} -lfcgi
Cflags: -I${includedir}/valum-@API_VERSION@
